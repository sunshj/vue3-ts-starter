{
  "stylelint.validate": ["css", "postcss", "scss", "vue"],
  "typescript.preferences.autoImportFileExcludePatterns": ["vue-router/auto$"],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue",
    "html",
    "markdown",
    "json",
    "jsonc",
    "yaml"
  ],
  "i18n-ally.localesPaths": ["src/i18n"],
  "i18n-ally.keystyle": "nested"
}
