{
  "name": "vue3-ts-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write src",
    "lint": "eslint src",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@vueuse/core": "^10.1.2",
    "axios": "^1.4.0",
    "darkreader": "^4.9.58",
    "dayjs": "^1.11.10",
    "echarts": "^5.4.2",
    "element-plus": "^2.3.5",
    "mavon-editor": "^3.0.1",
    "monaco-editor": "^0.39.0",
    "pinia": "^2.1.3",
    "vue": "^3.2.47",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@iconify-json/fa6-brands": "^1.1.11",
    "@iconify-json/fa6-solid": "^1.1.13",
    "@sunshj/eslint-config": "^1.0.1",
    "@sunshj/prettier-config": "^1.0.0",
    "@types/node": "^20.2.5",
    "@vitejs/plugin-vue": "^4.1.0",
    "commitlint": "^18.4.3",
    "cz-git": "^1.7.1",
    "eslint": "^8.41.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "typescript": "^5.0.4",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-icons": "^0.16.5",
    "unplugin-vue-components": "^0.25.0",
    "unplugin-vue-router": "^0.6.4",
    "vite": "^4.5.2",
    "vite-plugin-vue-layouts": "^0.9.1",
    "vue-tsc": "^1.4.2"
  },
  "prettier": "@sunshj/prettier-config",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "lint-staged": {
    "src/**/*.{vue,js,ts}": "eslint --fix"
  }
}
